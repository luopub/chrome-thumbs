{
  "manifest_version": 3,
  "name": "Tab Thumbnails - Visual Tab Manager",
  "version": "1.0",
  "created": "2025-12-19",
  "description": "A powerful Chrome extension that displays visual thumbnails of all open tabs. Switch between tabs quickly with visual previews, search, sort, and manage your browsing session efficiently.",
  "keywords": ["chrome extension", "tab management", "browser productivity", "tab thumbnails", "visual tab switcher", "tab navigation"],
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "homepage_url": "https://github.com/luopub/chrome-thumbs"
}